{
  "name": "astrosat-ui",
  "version": "0.0.1",
  "description": "A module containing a number of common UI widgets.",
  "main": "dist/js/index.cjs.js",
  "module": "dist/js/index.esm.js",
  "browser": "dist/js/index.umd.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/astrosat/astrosat-ui.git"
  },
  "keywords": [
    "mapbox",
    "mapbox-gl-js",
    "control",
    "modes"
  ],
  "files": [
    "dist"
  ],
  "author": "Mark Small",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/astrosat/astrosat-ui/issues"
  },
  "homepage": "https://github.com/astrosat/astrosat-ui#readme",
  "huskey": {
    "hooks": {
      "pre-commit": "yarn test",
      "pre-push": "yarn test"
    }
  },
  "jest": {
    "testRegex": "src/.*\\.test\\.(js|jsx)$"
  },
  "scripts": {
    "test": "jest --env=jsdom",
    "test:coverage": "yarn test -- --coverage",
    "lint": "eslint src/*.js",
    "clean": "rm -rf coverage dist",
    "flow": "flow check",
    "flowbuild": "flow-copy-source src dist",
    "build": "rollup -c",
    "watch": "rollup -c -w",
    "serve": "st -nc -i index.html",
    "semantic-release": "semantic-release --dryRun false --no-ci",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@semantic-release/changelog": "^3.0.1",
    "@semantic-release/commit-analyzer": "^6.1.0",
    "@semantic-release/git": "^7.0.5",
    "@semantic-release/github": "^5.2.1",
    "@semantic-release/npm": "^5.0.5",
    "@semantic-release/release-notes-generator": "^7.1.3",
    "@storybook/addon-actions": "^4.1.3",
    "@storybook/addon-knobs": "^5.2.1",
    "@storybook/addon-links": "^4.1.3",
    "@storybook/addons": "^4.1.3",
    "@storybook/react": "^4.1.3",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "eslint": "5.6.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-flowtype": "^3.0.0",
    "eslint-plugin-flowtype-errors": "^3.6.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-prettier": "^3.0.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "flow-bin": "^0.83.0",
    "flow-copy-source": "^2.0.2",
    "husky": "^1.1.2",
    "jest-cli": "^23.6.0",
    "mini-css-extract-plugin": "^0.5.0",
    "postcss-modules": "^1.4.1",
    "prettier": "^1.14.3",
    "react-styleguidist": "^8.0.6",
    "rollup": "^0.67.0",
    "rollup-plugin-babel": "^4.0.3",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-eslint": "^5.0.0",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-postcss": "^1.6.2",
    "rollup-watch": "^4.3.1",
    "semantic-release": "^15.10.7",
    "st": "^1.2.2",
    "svg-react-loader": "^0.4.6",
    "webpack": "^4.28.1"
  },
  "peerDependencies": {},
  "dependencies": {
    "prop-types": "^15.6.2",
    "react": "^16.6.3",
    "react-dom": "^16.6.3"
  }
}
