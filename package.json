{
  "name": "astrosat-ui",
  "version": "0.0.1",
  "description": "A module containing a number of common UI widgets.",
  "main": "dist/js/index.cjs.js",
  "module": "dist/js/index.esm.js",
  "browser": "dist/js/index.umd.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/astrosat/astrosat-ui.git"
  },
  "keywords": [
    "mapbox",
    "mapbox-gl-js",
    "control",
    "modes"
  ],
  "files": [
    "dist"
  ],
  "author": "Mark Small",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/astrosat/astrosat-ui/issues"
  },
  "homepage": "https://github.com/astrosat/astrosat-ui#readme",
  "jest": {
    "testRegex": "src/.*\\.test\\.(js|jsx)$"
  },
  "scripts": {
    "test": "jest --env=jsdom",
    "test:coverage": "yarn test -- --coverage",
    "lint": "eslint src/*.js",
    "clean": "rm -rf coverage dist",
    "flow": "flow check",
    "flowbuild": "flow-copy-source src dist",
    "build": "rollup -c",
    "watch": "rollup -c -w",
    "serve": "st -nc -i index.html",
    "release": "semantic-release --dryRun true --no-ci",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.0",
    "@babel/core": "^7.6.0",
    "@babel/preset-env": "^7.6.0",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@semantic-release/changelog": "^3.0.4",
    "@semantic-release/commit-analyzer": "^6.3.0",
    "@semantic-release/git": "^7.0.16",
    "@semantic-release/github": "^5.4.3",
    "@semantic-release/npm": "^5.1.15",
    "@semantic-release/release-notes-generator": "^7.3.0",
    "@storybook/addon-actions": "^5.2.1",
    "@storybook/addon-info": "^5.2.1",
    "@storybook/addon-knobs": "^5.2.1",
    "@storybook/addon-links": "^5.2.1",
    "@storybook/addons": "^5.2.1",
    "@storybook/react": "^5.2.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "eslint": "^6.4.0",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-flowtype": "^4.3.0",
    "eslint-plugin-flowtype-errors": "^4.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "flow-bin": "^0.107.0",
    "flow-copy-source": "^2.0.8",
    "jest-cli": "^24.9.0",
    "mini-css-extract-plugin": "^0.8.0",
    "postcss-modules": "^1.4.1",
    "prettier": "^1.18.2",
    "react-styleguidist": "^9.1.16",
    "rollup": "^1.21.4",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-eslint": "^7.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-watch": "^4.3.1",
    "semantic-release": "^15.13.24",
    "st": "^1.2.2",
    "svg-react-loader": "^0.4.6",
    "webpack": "^4.40.2"
  },
  "peerDependencies": {},
  "dependencies": {
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-svg-loader": "^3.0.3"
  }
}
