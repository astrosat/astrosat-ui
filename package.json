{
  "name": "@astrosat/astrosat-ui",
  "version": "2.5.0",
  "description": "A module containing a number of common UI widgets.",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/astrosat/astrosat-ui.git"
