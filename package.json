{
  "name": "@astrosat/astrosat-ui",
  "version": "1.8.4",
  "description": "A module containing a number of common UI widgets.",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/astrosat/astrosat-ui.git"
  },
  "keywords": [
    "mapbox",
    "mapbox-gl-js",
    "control",
    "modes"
  ],
  "files": [
    "dist"
  ],
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "author": "Mark Small",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/astrosat/astrosat-ui/issues"
  },
  "homepage": "https://github.com/astrosat/astrosat-ui#readme",
  "scripts": {
    "test": "react-scripts test --env=jsdom",
    "test:coverage": "yarn test -- --coverage",
    "lint": "eslint src/*.js",
    "clean": "rm -rf coverage dist node_modules",
    "build": "rollup -c",
    "watch": "rollup -c -w",
    "serve": "st -nc -i index.html",
    "release": "yarn clean && yarn && yarn build && semantic-release --no-ci",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.4",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-react": "^7.6.3",
    "@semantic-release/changelog": "^3.0.5",
    "@semantic-release/commit-analyzer": "^6.3.2",
    "@semantic-release/git": "^7.0.17",
    "@semantic-release/github": "^5.5.5",
    "@semantic-release/npm": "^5.3.4",
    "@semantic-release/release-notes-generator": "^7.3.2",
    "@storybook/addon-actions": "^5.2.5",
    "@storybook/addon-knobs": "^5.2.5",
    "@storybook/addon-links": "^5.2.5",
    "@storybook/addons": "^5.2.5",
    "@storybook/react": "^5.2.5",
    "@testing-library/jest-dom": "^4.2.3",
    "@testing-library/react": "^9.3.2",
    "@testing-library/react-hooks": "^3.1.1",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.2.0",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.1",
    "glob": "^7.1.6",
    "jest-cli": "^24.9.0",
    "mini-css-extract-plugin": "^0.8.0",
    "postcss-modules": "^1.4.1",
    "prettier": "^1.18.2",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-scripts": "^3.2.0",
    "react-styleguidist": "^10.1.0",
    "react-test-renderer": "^16.11.0",
    "rollup": "^1.26.3",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-eslint": "^7.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-svg": "^2.0.0",
    "semantic-release": "^15.13.30",
    "st": "^1.2.2"
  },
  "peerDependencies": {
    "react": "^16.11.0",
    "react-dom": "^16.11.0"
  },
  "dependencies": {
    "d3-scale": "^3.2.1",
    "prop-types": "^15.7.2",
    "react-range": "^1.4.3",
    "react-svg-loader": "^3.0.3",
    "zxcvbn": "^4.4.2"
  }
}
